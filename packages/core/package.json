{
  "name": "@vscode-schemas/core",
  "version": "0.1.0",
  "description": "",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@9.4.0",
  "license": "MIT",
  "homepage": "https://github.com/luxass/vscode-schemas",
  "repository": {
    "type": "git",
    "url": "https://github.com/luxass/vscode-schemas.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/luxass/vscode-schemas/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./patches": {
      "types": "./dist/patches/index.d.ts",
      "import": "./dist/patches/index.js"
    },
    "./patches/*": {
      "types": "./dist/patches/*.d.ts",
      "import": "./dist/patches/*.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf .turbo node_modules dist",
    "test": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "build-releases-type": "node scripts/build-releases-type.mjs"
  },
  "dependencies": {
    "tar": "^7.4.0",
    "vscode-uri": "^3.0.8",
    "which": "^4.0.0"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^4.7.0",
    "@types/tar": "^6.1.13",
    "@types/which": "^3.0.4",
    "eslint": "^9.6.0",
    "typescript": "^5.5.2"
  }
}
